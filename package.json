    "electron-store": "^5.0.0",
    "jspdf": "^2.3.1",
    "tailwindcss": "^1.1.2"
  },
  "devDependencies": {
    "electron-packager": "^14.0.4",
    "electron-reload": "^1.5.0"
  }
